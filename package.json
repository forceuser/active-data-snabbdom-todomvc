{
	"name": "active-data-snabbdom-todomvc",
	"version": "1.0.8",
	"description": "",
	"main": "index.js",
	"dependencies": {
		"active-data": "1.0.42",
		"keycode-js": "0.0.4",
		"snabbdom": "^0.6.9",
		"todomvc-app-css": "^2.1.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/forceuser/active-data-snabbdom-todomvc.git"
	},
	"va-release": {
		"files": [
			{
				"cwd": "templates/",
				"src": [
					"**/*"
				],
				"dest": "./"
			}
		]
	},
	"devDependencies": {
		"babel-core": "^6.24.0",
		"babel-eslint": "7.2.0",
		"babel-loader": "^7.0.0",
		"babel-plugin-istanbul": "^4.1.1",
		"babel-preset-es2015": "^6.24.0",
		"codecov": "^2.2.0",
		"documentation": "^4.0.0-rc.1",
		"eslint": "3.18.0",
		"eslint-plugin-babel": "4.1.1",
		"eslint-plugin-import": "2.2.0",
		"express": "^4.15.3",
		"faucet": "0.0.1",
		"globby": "^6.1.0",
		"http-server": "^0.10.0",
		"nyc": "^10.3.2",
		"publish-release": "^1.3.3",
		"semver": "^5.3.0",
		"shelljs": "^0.7.7",
		"sinon": "^2.2.0",
		"tape": "^4.6.3",
		"uglifyjs-webpack-plugin": "^0.4.1",
		"va-release": "^1.0.15",
		"webpack": "^2.5.1"
	},
	"scripts": {
		"test": "eslint .",
		"fix": "eslint --fix .",
		"coverage": "codecov",
		"release": "npm run patch-release",
		"patch-release": "npm test && va-release --version patch --github",
		"minor-release": "npm test && va-release --version minor --github",
		"major-release": "npm test && va-release --version major --github",
		"build-templates": "va-release -t",
		"build": "webpack --config ./build.config.js",
		"serve": "http-server -a localhost -p 8061"
	},
